  "version": "1.0.0",
  "description": "node challenge for tmg",
  "main": "src/main/server.ts",
  "scripts": {
    "start": "ts-node-dev --poll --inspect --transpile-only --ignore-watch node_modules --respawn src/main/server.ts",
